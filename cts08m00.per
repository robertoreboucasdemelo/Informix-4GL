attributes
   a2   = datmligacao.c24solnom  , noentry;

   b1   = datmservico.nom        , upshift
                                 , comments = " Nome";
   b2   = formonly.doctxt        , noentry;
   b6   = datmservico.corsus     , upshift
                                 , comments = " Susep";
   b7   = datmservico.cornom     , upshift
                                 , comments = " Corretor";
   b8   = formonly.cvnnom        , noentry;
   b9   = datmservico.vclcoddig  , format = "&&&&&&"
                                 , comments = " Codigo do veiculo";
   b10  = datmservico.vcldes     , upshift
                                 , comments = " Descricao do veiculo";
   b11  = datmservico.vclanomdl  , comments = " Ano do veiculo";
   b12  = datmservico.vcllicnum  , upshift
                                 , comments = " Placa do veiculo";
   b13  = formonly.vclcordes     , downshift
                                 , comments = " Cor do veiculo";

   c1  =  datmligacao.c24astcod  , noentry;
   c2  =  formonly.c24astdes     , noentry;

   f1  =  formonly.endereco      , upshift;
   f2  =  gsakend.endbrr         , upshift;
   f3  =  gsakend.endcid         , upshift;
   f4  =  gsakend.endufd         , upshift;
   f5  =  gsakend.endcep         ;
   f6  =  gsakend.endcepcmp      ;
