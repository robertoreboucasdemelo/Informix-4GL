                                 , comments = " Confirma solicitacao: (S)im ou (N)ao";
   f8  = gsakend.endlgdtip       , upshift;
   f9  = datmsegviaend.endnum    ;
   f10 = datmsegviaend.endcmp    , upshift;
end

instructions
   delimiters " "
end
