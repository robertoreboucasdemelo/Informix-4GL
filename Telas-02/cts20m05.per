   a1   = datmservico.cnldat                    ,noentry;
   a2   = datmservico.atdfnlhor                 ,noentry;
   a3   = formonly.funnom                       ,noentry;
   c    = datketapa.atdetpcod                   ,noentry;
   c1   = datketapa.atdetpdes         ,reverse  ,noentry;
   c2   = datmservico.atdprscod       ,format = "&&&&&&";
   c3   = dpaksocor.nomgrr                      ,noentry;
   c4   = formonly.cidufdprs                    ,noentry;
   c5   = dpaksocor.dddcod                      ,noentry;
   c6   = dpaksocor.teltxt                      ,noentry;
   c7   = datmservico.c24nomctt                 ,noentry;
   c8   = datmservico.atdvclsgl                 ,noentry;
   c9   = datmservico.socvclcod                 ,noentry;
   c10  = formonly.vcldes                       ,noentry;
   c11  = datmservico.srrcoddig                 ,noentry;
   c12  = datksrr.srrabvnom                     ,noentry;
 end

 instructions
   delimiters " "
 end
