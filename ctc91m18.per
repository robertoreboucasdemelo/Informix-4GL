    k  = formonly.rgcflg                 , upshift, comments = " Regulacao Imagem? (S/N)";
    l  = formonly.h24rcbflg              , upshift, comments = " Recebimento 24 horas de veiculos? (S/N)";
    m  = formonly.lotada                 , upshift, comments = " Oficina Lotada? (S/N)";
    n  = formonly.ltalibdat              , upshift, comments = " Data previsao de liberacao";
    o1 = formonly.atldat                 , upshift, noentry;
    o2 = formonly.atlfunnom              , upshift, noentry;
    l1 = formonly.label1                 , upshift, noentry;
