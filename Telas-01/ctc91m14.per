    a1 = formonly.atdcencod   , upshift, comments = " Codigo do CAR";
    a2 = formonly.atdcennom   , upshift, comments = " Nome do CAR";
    a  = formonly.atoflg      , upshift, comments = " C.A.R. Ativo ? (S/N)";
