tables
  datmassistpassag,
  datmservico,
  datmlcl

attributes

a1   = formonly.servico               , noentry;
a3   = formonly.prpnumdsp             , noentry;
a2   = formonly.c24solnom             , noentry;

b1   = formonly.nom                   , upshift
                                      , comments = " Nome";
b2   = formonly.nscdat type date, upshift,
       comments = " Informe a data de Nascimento ";
b3   = formonly.srvpedcod, noentry, reverse;
b6   = formonly.corsus                , upshift
                                      , comments = " Susep";
b7   = formonly.cornom                , upshift
                                      , noentry;
b9   = datmservico.vclcoddig          , format = "&&&&&&"
                                      , comments = " Codigo do veiculo";
b10  = formonly.vcldes                , upshift
                                      , comments = " Descricao do veiculo";
b11  = formonly.vclanomdl             , comments = " Ano do veiculo";
b12  = formonly.vcllicnum             , upshift
                                      , comments = " Placa do veiculo";
b13  = formonly.vclcordes             , downshift
                                      , comments = " Cor do veiculo";

c0   = formonly.asitipcod             , comments = " Tipo de assistencia";
c1   = formonly.asitipabvdes          , noentry;
c    = formonly.asimtvcod             , comments = " Motivo da solicitacao";
c2   = formonly.asimtvdes             , upshift;
c3   = formonly.refatdsrvorg type dec , format = "&&"
                                      , comments = " Origem do servico de referencia";
c4   = datmassistpassag.refatdsrvnum, format = "&&&&&&&"
                                      , comments = " Numero do servico de referencia";
c5   = datmassistpassag.refatdsrvano, format = "&&"
                                      , comments = " Ano do servico de referencia";

d1   = formonly.lgdtxt                , upshift
                                      , comments = " Local onde se encontram os passageiros";
d2   = formonly.lclbrrnom             , upshift;
d3   = formonly.cidnom                , upshift;
d4   = formonly.ufdcod                , upshift;
d5   = formonly.lclrefptotxt          , upshift
                                      , comments = " Ponto de referencia do local";
d6   = formonly.endzon                , upshift
                                      , comments = " Zona: (NO)rte, (SU)l, (LE)ste, (OE)ste ou (CE)ntral";
d7   = formonly.dddcod                , comments = " DDD do local onde se encontram os passageiros";
d8   = formonly.lcltelnum             , comments = " Telefone do local onde se encontram os passageiros";
d9   = formonly.lclcttnom             , upshift
                                      , comments = " Responsavel no local";
d10  = datmlcl.endcmp                 , upshift
                                      ,comments = " Complemento do endereco";
d11  = datmlcl.celteldddcod           ,comments = " Codigo DDD do Celular";
d12  = datmlcl.celtelnum              ,comments = " Telefone Celular";
e0   = formonly.dstlcl                , upshift
                                      , comments = " Local de destino dos passageiros";
e1   = formonly.dstlgdtxt             , upshift;
e2   = formonly.dstbrrnom             , upshift;
e3   = formonly.dstcidnom             , upshift;
e4   = formonly.dstufdcod             , upshift;
f    = formonly.imdsrvflg             , upshift
                                      , comments = " Servico imediato ? (S)im ou (N)ao";
g    = formonly.atdprinvlcod          , comments = " Nivel de Prioridade no atendimento: (1)-Baixa, (2)-Normal ou (3)-Urgente";
g1   = formonly.atdprinvldes          , noentry;
h    = formonly.atdlibflg             , upshift
                                      , comments = " Servico liberado ? (S)im ou (N)ao";
j1   = formonly.atdtxt                , noentry;
j2   = formonly.atdlibdat             , noentry;
j3   = formonly.atdlibhor             , noentry;
k    = formonly.msgfun                , noentry;
waz  = formonly.msg_pss type char, noentry;
end

instructions
  delimiters " "
end
